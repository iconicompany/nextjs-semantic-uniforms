{
  "name": "nextjs-semantic",
  "description": "Next.js + Fomantic-UI + Styled Components + Uniforms",
  "version": "4.0.1",
  "author": "Mart√≠n M.",
  "license": "MIT",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "now-build": "npm run semantic:build && next build",
    "semantic:build": "cd .semantic && gulp build-css build-assets",
    "semantic:watch": "cd .semantic && npm run semantic:build && gulp watch",
    "clean": "rimraf --no-glob ./.next ./out",
    "check:es:build": "es-check es5 './.next/static/**/*.js' -v",
    "escheck": "npm run clean && npm run build && npm run check:es:build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest"
  },
  "dependencies": {
    "@zeit/next-css": "^1.0.1",
    "ajv": "^7.1.0",
    "ajv-formats": "^1.5.1",
    "ajv-i18n": "^3.6.0",
    "next": "^10.0.6",
    "next-compose-plugins": "^2.2.1",
    "next-transpile-modules": "^6.3.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hook-form": "^6.15.1",
    "semantic-ui-react": "^2.0.3",
    "styled-components": "^5.2.1",
    "uniforms": "^3.2.1",
    "uniforms-bridge-json-schema": "^3.2.1",
    "uniforms-semantic": "^3.2.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "es-check": "^5.2.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "fomantic-ui": "^2.8.7",
    "inquirer": "^7.3.3",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "url-loader": "^4.1.1"
  }
}
